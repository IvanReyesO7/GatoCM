<h1><%= @list.name %></h1> 
<% @list.items.each do |item| %>
  <ul class="list-group">
    <li class="list-group-item"><%= item.content %></li>
  </ul>
<% end %>
<div class="mt-3">
  <%= form_for @list, url: user_application_list_import_url(list_name_format: @list.name_format), html: { multipart: true }, method: :post do |f| %>
    <%= f.file_field :uploaded_file %>
    <%= f.submit "Upload" %>
  <% end %>
  <%= button_to "Create Item", new_user_application_list_item_url(list_name_format: @list.name_format), method: :get %>
  <%= button_to "Back", user_application_path(name: @application.name), method: :get, class: "btn btn-dark mt-3" %>
</div>