<div class="navbar">
  <%=link_to image_tag("logo.png", alt: "GatoCmM's logo", class: "logo nav-child"), "#"%>
  <div class="session-links nav-child">
    <% if current_user %>
      [Logged as: <%= link_to current_user.username, user_applications_index_path(user_username: current_user.username), :method => :get %>]
      <%= link_to '[Sign out]', destroy_user_session_path, :method => :delete %>
    <% else %>
      <%= link_to '[Sign up]', new_user_registration_path, :method => :get %>
      <%= link_to '[Sign in]', new_user_session_path, :method => :new %>
    <% end %>
  </div>
</div>